\lhead{Appendix \label{AppendixA}. \emph{Convergence plots}} % Change X to a consecutive letter; this is for the header on each page - perhaps a shortened title

This appendix contains the convergence plots of the experiments in \cref{chapter:experiments}.

\begin{figure}[h!]
  \centering
  \subfigure[\label{subfig:conI1}]{\includegraphics[width=0.23\textwidth]{I1con.eps}}
  ~
  \subfigure[\label{subfig:con2I1}]{\includegraphics[width=0.23\textwidth]{I1con2.eps}}
  ~
  \subfigure[\label{subfig:concvI1}]{\includegraphics[width=0.23\textwidth]{I1concv.eps}}        
  ~
  \subfigure[\label{subfig:con2cvI1}]{\includegraphics[width=0.23\textwidth]{I1con2cv.eps}}
  \caption{Convergence plots of figure \ref{I1}. \ref{subfig:conI1} and \ref{subfig:concvI1} show the respective functionals $F$ of OL and CVL, while \ref{subfig:con2I1} and \ref{subfig:con2cvI1} show the difference $||\phi^{n+1}-\phi^n||$ of OL and CVL.}\label{I1con}
\end{figure}

